{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "components": "./components",
            "containers": "./containers",
            "queries": "./graphql/queries"
          }
        }]
      ]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": ["react", "env", "stage-0"],
      "plugins": [
        "require-context-hook"
      ]
    }
  }
}
